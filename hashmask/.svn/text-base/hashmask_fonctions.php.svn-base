//
// ajout CSS et JS
//
function hashmask_insert_head($flux){
        $flux .= '<link rel="stylesheet" type="text/css" href="'.find_in_path('hashmask.css').'" media="all" />'."\n";

        $flux .= '<script src="'.find_in_path('hashmask.sha1.js').'" type="text/javascript"></script>'."\n";;
        $flux .= '<script src="'.find_in_path('hashmask.sparkline.js').'" type="text/javascript"></script>'."\n";;
        $flux .= '<script src="'.find_in_path('hashmask.js').'" type="text/javascript"></script>'."\n";;
        
        return $flux;
}


